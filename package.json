{
  "name": "express-api",
  "version": "1.0.0",
  "description": "server api CoinGecko",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "copy-dot-env": "ts-node copyDotEnv.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intec-co/express-api.git"
  },
  "author": "cavargasp@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/intec-co/express-api/issues"
  },
  "homepage": "https://github.com/intec-co/express-api#readme",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/coingecko-api": "^1.0.2",
    "@types/eslint": "^7.28.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.6.0",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.3.4",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.1",
    "coingecko-api": "^1.0.10",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "inversify": "^5.1.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.0",
    "mongodb-memory-server": "^7.3.6",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.3.3"
  }
}